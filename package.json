{
  "name": "hast-util-script-supporting",
  "version": "1.0.0",
  "description": "Check if a node is a script-supporting element",
  "license": "MIT",
  "keywords": [
    "hast",
    "html",
    "category",
    "script",
    "supporting",
    "util",
    "utility"
  ],
  "repository": "https://github.com/syntax-tree/hast-util-script-supporting",
  "bugs": "https://github.com/syntax-tree/hast-util-script-supporting/issues",
  "author": "Titus Wormer <tituswormer@gmail.com> (http://wooorm.com)",
  "contributors": [
    "Titus Wormer <tituswormer@gmail.com> (http://wooorm.com)"
  ],
  "files": [
    "index.js"
  ],
  "dependencies": {
    "hast-util-is-element": "^1.0.0"
  },
  "devDependencies": {
    "browserify": "^14.0.0",
    "esmangle": "^1.0.1",
    "istanbul": "^0.4.0",
    "remark-cli": "^3.0.0",
    "remark-preset-wooorm": "^2.0.0",
    "tape": "^4.4.0",
    "xo": "^0.18.0"
  },
  "scripts": {
    "build-md": "remark . --quiet --frail --output",
    "build-bundle": "browserify index.js --bare -s hastUtilScriptSupporting > hast-util-script-supporting.js",
    "build-mangle": "esmangle hast-util-script-supporting.js > hast-util-script-supporting.min.js",
    "build": "npm run build-md && npm run build-bundle && npm run build-mangle",
    "lint": "xo",
    "test-api": "node test.js",
    "test-coverage": "istanbul cover test.js",
    "test": "npm run build && npm run lint && npm run test-coverage"
  },
  "xo": {
    "space": true,
    "esnext": false,
    "ignores": [
      "hast-util-is-element.js"
    ]
  },
  "remarkConfig": {
    "plugins": [
      "preset-wooorm"
    ]
  }
}
